{
  "name": "Sara-Bot",
  "description": "A Telegram Bot By Akku",
  "keywords": [
    "telegram",
    "sarabot",
    "plugin",
    "telegram-sarabot",
    "productivity"
  ],
  "repository": "https://github.com/AkkuPY/Sara-Bot",
  "env": {
    "ENV": {
      "description": "Setting this to ANYTHING will enable heroku.",
      "value": "ANYTHING"
    },
    "bot_token": {
      "description": "Enter Telegram Bot Token Here ",
      "value": ""
    },
    "bot_user_name": {
      "description": "Enter Bot Username Here",
      "value": ""
    },
    "URL": {
      "description": "Enter Bot URL here(Replace <app-name> with app name given on top most)",
      "value": "https://<app-name>.herokuapp.com/"
    }
  },
  "addons": [
    {
      "plan": "heroku-postgresql",
      "options": {
        "version": "12"
      }
    }
  ]
}